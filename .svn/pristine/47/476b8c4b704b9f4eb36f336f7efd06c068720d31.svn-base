package com.ihc.ehr.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class ORMCCDRequest {
	
	@Id
	private String patient_id;
	private String chart_id;
	private String provider_id;
	private String practice_id;
	private Boolean isReferal ;
	private String ccd_Version;
	private String ccd_type;
	
	public String getPatient_id() {
		return patient_id;
	}
	public void setPatient_id(String patient_id) {
		this.patient_id = patient_id;
	}
	public String getChart_id() {
		return chart_id;
	}
	public void setChart_id(String chart_id) {
		this.chart_id = chart_id;
	}
	public String getProvider_id() {
		return provider_id;
	}
	public void setProvider_id(String provider_id) {
		this.provider_id = provider_id;
	}
	public String getPractice_id() {
		return practice_id;
	}
	public void setPractice_id(String practice_id) {
		this.practice_id = practice_id;
	}
	public Boolean getIsReferal() {
		return isReferal;
	}
	public void setIsReferal(Boolean isReferal) {
		this.isReferal = isReferal;
	}
	public String getCcd_Version() {
		return ccd_Version;
	}
	public void setCcd_Version(String ccd_Version) {
		this.ccd_Version = ccd_Version;
	}
	public String getCcd_type() {
		return ccd_type;
	}
	public void setCcd_type(String ccd_type) {
		this.ccd_type = ccd_type;
	}
	
	

}
