package com.ihc.ehr.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class ORMGetPatientConsultSummary {
	@Id
	private Long consultation_id;
	private String consult_date;
	private String consultant_name;
	private String specility;
	private String clinic_name;
	private String reason;
	public Long getConsultation_id() {
		return consultation_id;
	}
	public void setConsultation_id(Long consultation_id) {
		this.consultation_id = consultation_id;
	}
	public String getConsult_date() {
		return consult_date;
	}
	public void setConsult_date(String consult_date) {
		this.consult_date = consult_date;
	}
	public String getConsultant_name() {
		return consultant_name;
	}
	public void setConsultant_name(String consultant_name) {
		this.consultant_name = consultant_name;
	}
	public String getSpecility() {
		return specility;
	}
	public void setSpecility(String specility) {
		this.specility = specility;
	}
	public String getClinic_name() {
		return clinic_name;
	}
	public void setClinic_name(String clinic_name) {
		this.clinic_name = clinic_name;
	}
	public String getReason() {
		return reason;
	}
	public void setReason(String reason) {
		this.reason = reason;
	}

}
