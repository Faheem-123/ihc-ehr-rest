package com.ihc.ehr.model;

import java.util.List;

public class SearchCriteria {
	
	Long practice_id;
	List<SearchCriteriaParamList> param_list;
	String criteria;
	String option;	
	
	public Long getPractice_id() {
		return practice_id;
	}
	public void setPractice_id(Long practice_id) {
		this.practice_id = practice_id;
	}
	public String getCriteria() {
		return criteria;
	}
	public void setCriteria(String criteria) {
		this.criteria = criteria;
	}
	public String getOption() {
		return option;
	}
	public void setOption(String option) {
		this.option = option;
	}
	public List<SearchCriteriaParamList> getParam_list() {
		return param_list;
	}
	public void setParam_list(List<SearchCriteriaParamList> param_list) {
		this.param_list = param_list;
	}
	@Override
	public String toString() {
		return "SearchCriteria [practice_id=" + practice_id + ", param_list=" + param_list + ", criteria=" + criteria
				+ ", option=" + option + "]";
	}
	
	
	
	
}
