package com.ihc.ehr.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class ORMCashRegisterPatInfo {

	@Id
	private Long patient_id;
	private String ins_name;
	private String copay;
	private String prev_balance;
	private String patient_advance;
	private String self_pay_due;
	public Long getPatient_id() {
		return patient_id;
	}
	public void setPatient_id(Long patient_id) {
		this.patient_id = patient_id;
	}
	public String getIns_name() {
		return ins_name;
	}
	public void setIns_name(String ins_name) {
		this.ins_name = ins_name;
	}
	public String getCopay() {
		return copay;
	}
	public void setCopay(String copay) {
		this.copay = copay;
	}
	public String getPrev_balance() {
		return prev_balance;
	}
	public void setPrev_balance(String prev_balance) {
		this.prev_balance = prev_balance;
	}
	public String getPatient_advance() {
		return patient_advance;
	}
	public void setPatient_advance(String patient_advance) {
		this.patient_advance = patient_advance;
	}
	public String getSelf_pay_due() {
		return self_pay_due;
	}
	public void setSelf_pay_due(String self_pay_due) {
		this.self_pay_due = self_pay_due;
	}
	
	
	
	
}
